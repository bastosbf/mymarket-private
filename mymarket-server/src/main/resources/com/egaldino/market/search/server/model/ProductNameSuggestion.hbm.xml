<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.egaldino.market.search.server.model.ProductNameSuggestion"
		table="product_name_suggestion">
		<id name="id" column="id">
			<generator class="identity" />
		</id>
		<property name="currentName" column="current_name"/>
		<property name="suggestedName" column="suggested_name"/>
		<property name="barcode" />
		<property name="date" />
	</class>
</hibernate-mapping>