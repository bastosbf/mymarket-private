<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.mymarket.server.model.ProductNameSuggestion" table="product_name_suggestion">
		<id name="id" column="id">
			<generator class="identity" />
		</id>
		<property name="name"/>
		<property name="barcode" />
		<many-to-one name="market" class="com.mymarket.server.model.Market" fetch="select" />
		<property name="price" />
		<property name="status" />
		<property name="date" />
	</class>
</hibernate-mapping>